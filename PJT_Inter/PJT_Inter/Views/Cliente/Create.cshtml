@model Cliente
<h2>Cadastro - Cliente</h2>

<form asp-controller="Cliente" asp-action="Create" method="post" ng-app="">
    <hr />
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" placeholder="Nome" />
        <span asp-validation-for="Nome"></span>
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" placeholder="E-Mail" />
        <span asp-validation-for="Email"></span>
    </div>
    <div class="form-group">
        <label>Endereço</label>
        <input asp-for="Endereco" class="form-control" placeholder="Endereço" />
        <span asp-validation-for="Endereco"></span>
    </div>

    <div class="form-group">
        <div class="form-row">
            <div class="col">
                <label asp-for="CPF_CNPJ"></label>
                <input asp-for="CPF_CNPJ" class="form-control" placeholder="CPF/CNPJ">
                <span asp-validation-for="CPF_CNPJ"></span>
            </div>
            <div class="col">
                <label asp-for="Telefone"></label>
                <input asp-for="Telefone" class="form-control" placeholder="Telefone">
                <span asp-validation-for="Telefone"></span>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="form-check form-check-inline">
            <input asp-for="Status" checked type="radio" name="status" id="inlineCheckbox1" value="1">
            <span asp-validation-for="Status">Ativo</span>
        </div>
    </div>
    <h1 {{Heelo World}}></h1>
    <button class="btn btn-primary ">Salvar</button>
</form>
